<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
    android:id="@+id/appBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:elevation="3dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#EFF1F3"

        app:title="Keperluan Pribadi"
        android:elevation="1dp"
        android:foregroundTint="@color/black"
        app:navigationIcon="@drawable/arrow_left"
        app:titleTextColor="@color/black" />

</com.google.android.material.appbar.AppBarLayout>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/prwFashion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Fashion Wanita"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prpFashion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Fashion Pria"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prhJam"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Jam Tangan"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prcOlahraga"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Pakaian Olahraga"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prHias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Perhiasan"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prParfum"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Make Up &amp; Parfum"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prObat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Terapi &amp; Pengobatan"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prRawat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Perawatan"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prNutrisi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Nutrisi &amp; Suplemen"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/prOthers"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="15dp"
            android:text="Lainnya"
            android:textColor="@color/black"
            android:textSize="18sp" />
        </LinearLayout>
</LinearLayout>